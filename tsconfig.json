{
  "compilerOptions": {
    "target": "es5",                                
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@routes/*": ["routes/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
